// DSPL data for the gallery images
const galleryData = [
  {
    name: "Jungle Waterfall",
    src: "C:\\Users\\MRUDI\\Downloads\\waterfal.jpg",
    description: "This stunning waterfall is hidden deep within the jungle. It's the perfect place to cool off after a long hike."
  },
  {
    name: "Majestic Tiger",
    src: "C:\\Users\\MRUDI\\Downloads\\tig.jpg",
    description: "Spotting a tiger in the wild is a rare and unforgettable experience. Our guides are experts in tracking these elusive creatures."
  },
  {
    name: "Vibrant Parrots",
    src: "C:\\Users\\MRUDI\\Downloads\\par.jpg",
    description: "The jungle is home to some of the most beautiful and colorful birds in the world, like these vibrant parrots."
  },
  {
       name: "Ancient ruins",
    src: "C:\Users\\MRUDI\\Downloads\\fores.jpg",
    description: "The jungle is full of mysteries, like these ancient ruins that have been reclaimed by nature."
  }
];

// Function to create gallery items from DSPL data
function createGalleryItems(data) {
  let items = "";
  data.forEach(item => {
    items += `
      <div class="gallery-item">
        <img src="${item.src}">
        <div class="gallery-item-overlay">
          <h3>${item.name}</h3>
          <p>${item.description}</p>
        </div>
      </div>
    `;
  });
  return items;
}

// Add gallery items to the gallery container
const galleryContainer = document.querySelector(".gallery-container");
galleryContainer.innerHTML = createGalleryItems(galleryData);



</script>


